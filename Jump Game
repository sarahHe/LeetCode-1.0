class Solution {
public:
    bool canJump(int A[], int n) {
        int sum = 0;
        vector<int> zero;
        for (int i = 0; i < n-1; i++) {
            if (!A[i]) { 
                int j = i - 1;
                while (j >=0) {
                    if (A[j] > i - j)
                        break;
                    j--;
                }
                if (j < 0)
                    return false;
            }
        }
        return true;
    }
};
